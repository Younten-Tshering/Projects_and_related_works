<%= content_tag "div", id: "upvote-#{event.id}" do %>
    <%= link_to upvote_event_path(event), method: :patch, remote: true, class: "btn btn-primary btn-sm" do %>
        <% if current_user.voted_up_on? event %>
            UnLike
        <% else %>
            Like
        <% end %>
    <% end %>
<% end %>